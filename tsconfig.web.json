{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.tsx",
    "src/preload/*.d.ts",
    "src/common/**/*",
    "src/types/**/*"
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "paths": {
      "@renderer/*": ["renderer/src/*"],
      "@common/*": ["common/*"],
      "@launcherTypes/*": ["types/*"]
    }
  }
}
